{
    "aur": {
        "title": "Arch Linux (AUR)",
        "link_template": "https://aur.archlinux.org/packages/{{value}}",
        "command_template": "sudo pacaur -S {{value}}"
    },
    "brew_cask": {
        "title": "Homebrew",
        "link_template": "https://formulae.brew.sh/cask/{{value}}",
        "command_template": "brew install --cask {{value}}"
    },
    "brew_formula": {
        "title": "Homebrew",
        "link_template": "https://formulae.brew.sh/formula/{{value}}",
        "command_template": "brew install {{value}}"
    },
    "debian": {
        "title": "Debian",
        "link_template": "https://packages.debian.org/stable/{{value}}",
        "command_template": "sudo apt install {{value}}"
    },
    "docker_hub": {
        "title": "DockerHub",
        "link_template": "https://hub.docker.com/{{value}}",
        "command_template": "docker pull docker.io/{{value}}"
    },
    "docker_ghcr": {
        "title": "GitHub Container Registry",
        "link_template": "https://ghcr.io/{{value}}",
        "command_template": "docker pull ghcr.io/{{value}}"
    },
    "gem": {
        "title": "Ruby Gem",
        "link_template": "https://rubygems.org/gems/{{value}}",
        "command_template": "gem install {{value}}"
    },
    "github": {
        "title": "Github",
        "link_template": "https://github.com/{{value}}",
        "command_template": "git clone https://github.com/{{value}}.git"
    },
    "gitlab": {
        "title": "Gitlab",
        "link_template": "https://gitlab.com/{{value}}",
        "command_template": "git clone https://gitlab.com/{{value}}"
    },
    "kali": {
        "title": "Kali Linux",
        "link_template": "https://pkg.kali.org/pkg/{{value}}",
        "command_template": "sudo apt install {{value}}"
    },
    "npm": {
        "title": "npm",
        "link_template": "https://www.npmjs.com/package/{{value}}",
        "command_template": "npm install {{value}}"
    },
    "pacman": {
        "title": "Arch Linux",
        "link_template": "https://archlinux.org/packages/?name={{value}}",
        "command_template": "sudo pacman -S {{value}}"
    },
    "pypi": {
        "title": "PyPI",
        "link_template": "https://pypi.org/project/{{value}}",
        "command_template": "pip install {{value}}"
    },
    "pip_github": {
        "title": "Python package (Github)",
        "link_template": "https://github.com/{{value}}",
        "command_template": "pip install git+https://github.com/{{value}}"
    },
    "pip_gitlab": {
        "title": "Python package (Gitlab)",
        "link_template": "https://gitlab.com/{{value}}",
        "command_template": "pip install git+https://gitlab.com/{{value}}"
    },
     "winget": {
        "title": "Winget",
        "link_template": "https://winstall.app/apps/{{value}}",
        "command_template": "winget install --id {{value}} -e"
    }
}